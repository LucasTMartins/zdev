<mvc:View&nbsp;                                                                                                                                                                                                                                                
    controllerName="com.lit.zui5.material.deposito.controller.View1"                                                                                                                                                                                           
    xmlns:mvc="sap.ui.core.mvc" displayBlock="true"                                                                                                                                                                                                            
    xmlns="sap.m"                                                                                                                                                                                                                                              
    xmlns:core="sap.ui.core"                                                                                                                                                                                                                                   
    xmlns:semantic="sap.f.semantic">                                                                                                                                                                                                                           
    <Page id="page" title="{i18n>title}">                                                                                                                                                                                                                      
        <IconTabBar id="iconTabBar" select=".onQuickFilter" expandable="false">                                                                                                                                                                                
            <items>                                                                                                                                                                                                                                            
                <IconTabFilter&nbsp;                                                                                                                                                                                                                           
                    id="_IDGenIconTabFilter1"&nbsp;                                                                                                                                                                                                            
                    key="todos"&nbsp;                                                                                                                                                                                                                          
                    showAll="true"&nbsp;                                                                                                                                                                                                                       
                    count="{view1View>/todos}"&nbsp;                                                                                                                                                                                                           
                    text="Todos" />                                                                                                                                                                                                                            
                <IconTabSeparator id="_IDGenIconTabSeparator1" />                                                                                                                                                                                              
                <IconTabFilter&nbsp;                                                                                                                                                                                                                           
                    id="_IDGenIconTabFilter2"&nbsp;                                                                                                                                                                                                            
                    class="iconBold"                                                                                                                                                                                                                           
                    key="aprovados"                                                                                                                                                                                                                            
                    icon="sap-icon://accept"                                                                                                                                                                                                                   
                    iconColor="Positive"&nbsp;                                                                                                                                                                                                                 
                    count="{view1View>/aprovados}"                                                                                                                                                                                                             
                    text="Aprovados"&nbsp;                                                                                                                                                                                                                     
                    design="Horizontal" />                                                                                                                                                                                                                     
                <IconTabFilter&nbsp;                                                                                                                                                                                                                           
                    id="_IDGenIconTabFilter3"&nbsp;                                                                                                                                                                                                            
                    key="pendentes"                                                                                                                                                                                                                            
                    icon="sap-icon://BusinessSuiteInAppSymbols/icon-priority-1"                                                                                                                                                                                
                    iconColor="Critical"&nbsp;                                                                                                                                                                                                                 
                    count="{view1View>/pendentes}"                                                                                                                                                                                                             
                    text="Pendentes"&nbsp;                                                                                                                                                                                                                     
                    design="Horizontal" />                                                                                                                                                                                                                     
                <IconTabFilter&nbsp;                                                                                                                                                                                                                           
                    id="_IDGenIconTabFilter4"&nbsp;                                                                                                                                                                                                            
                    key="reprovados"                                                                                                                                                                                                                           
                    icon="sap-icon://decline"                                                                                                                                                                                                                  
                    iconColor="Negative"&nbsp;                                                                                                                                                                                                                 
                    count="{view1View>/reprovados}"                                                                                                                                                                                                            
                    text="Reprovados"&nbsp;                                                                                                                                                                                                                    
                    design="Horizontal" />                                                                                                                                                                                                                     
                <IconTabSeparator id="_IDGenIconTabSeparator2" />                                                                                                                                                                                              
                <IconTabFilter&nbsp;                                                                                                                                                                                                                           
                    id="_IDGenIconTabFilter5"&nbsp;                                                                                                                                                                                                            
                    key="ativos"                                                                                                                                                                                                                               
                    icon="sap-icon://accept"                                                                                                                                                                                                                   
                    iconColor="Default"&nbsp;                                                                                                                                                                                                                  
                    count="{view1View>/ativos}"                                                                                                                                                                                                                
                    text="Ativos"&nbsp;                                                                                                                                                                                                                        
                    design="Horizontal" />                                                                                                                                                                                                                     
                <IconTabFilter&nbsp;                                                                                                                                                                                                                           
                    id="_IDGenIconTabFilter6"&nbsp;                                                                                                                                                                                                            
                    key="inativos"                                                                                                                                                                                                                             
                    icon="sap-icon://decline"                                                                                                                                                                                                                  
                    iconColor="Neutral"&nbsp;                                                                                                                                                                                                                  
                    count="{view1View>/inativos}"                                                                                                                                                                                                              
                    text="Inativos"&nbsp;                                                                                                                                                                                                                      
                    design="Horizontal" />                                                                                                                                                                                                                     
            </items>                                                                                                                                                                                                                                           
        </IconTabBar>                                                                                                                                                                                                                                          
        <Table id="listMain"                                                                                                                                                                                                                                   
            width="auto"                                                                                                                                                                                                                                       
            items="{/zlmot021Set}"                                                                                                                                                                                                                             
            updateFinished=".onListUpdateFinished"                                                                                                                                                                                                             
            noDataText="{i18n>detailLineItemTableNoDataText}"                                                                                                                                                                                                  
            busyIndicatorDelay="{detailView>/lineItemTableDelay}"                                                                                                                                                                                              
            mode="SingleSelectLeft"                                                                                                                                                                                                                            
            >                                                                                                                                                                                                                                                  
            <headerToolbar>                                                                                                                                                                                                                                    
                <OverflowToolbar id="overflowToolbarList">                                                                                                                                                                                                     
                    <SearchField                                                                                                                                                                                                                               
                        id="searchFieldList"                                                                                                                                                                                                                   
                        showRefreshButton="true"                                                                                                                                                                                                               
                        tooltip="{i18n>listSearchTooltip}"                                                                                                                                                                                                     
                        search=".onSearch"                                                                                                                                                                                                                     
                        width="auto">                                                                                                                                                                                                                          
                        <layoutData>                                                                                                                                                                                                                           
                            <OverflowToolbarLayoutData&nbsp;                                                                                                                                                                                                   
                                id="overflowToolbarLayoutDataList"                                                                                                                                                                                             
                                minWidth="150px"                                                                                                                                                                                                               
                                maxWidth="240px"                                                                                                                                                                                                               
                                shrinkable="true"                                                                                                                                                                                                              
                                priority="NeverOverflow"/>                                                                                                                                                                                                     
                        </layoutData>                                                                                                                                                                                                                          
                    </SearchField>                                                                                                                                                                                                                             
                                                                                                                                                                                                                                                               
                    <ToolbarSpacer id="toolbarSpacer1" />                                                                                                                                                                                                      
                                                                                                                                                                                                                                                               
                    <Button                                                                                                                                                                                                                                    
                        id="approveMaterialBtn"                                                                                                                                                                                                                
                        text="Aprovar"                                                                                                                                                                                                                         
                        press=".onBtnApproveOrRepprovePress('A')"&nbsp;                                                                                                                                                                                        
                        icon="sap-icon://accept"                                                                                                                                                                                                               
                        type="Accept"/>                                                                                                                                                                                                                        
                    <Button                                                                                                                                                                                                                                    
                        id="RepproveMaterialBtn"                                                                                                                                                                                                               
                        text="Reprovar"                                                                                                                                                                                                                        
                        press=".onBtnApproveOrRepprovePress('R')"                                                                                                                                                                                              
                        icon="sap-icon://decline"                                                                                                                                                                                                              
                        type="Reject"/>                                                                                                                                                                                                                        
                    <Button                                                                                                                                                                                                                                    
                        id="createMaterialBtn"                                                                                                                                                                                                                 
                        press=".onBtnCreatePress"&nbsp;                                                                                                                                                                                                        
                        icon="sap-icon://create"                                                                                                                                                                                                               
                        type="Success"/>                                                                                                                                                                                                                       
                    <Button                                                                                                                                                                                                                                    
                        id="deleteMaterialBtn"                                                                                                                                                                                                                 
                        press=".onBtnDeletePress"                                                                                                                                                                                                              
                        icon="sap-icon://delete"                                                                                                                                                                                                               
                        type="Negative"/>                                                                                                                                                                                                                      
                    <Button                                                                                                                                                                                                                                    
                        id="sortButton"                                                                                                                                                                                                                        
                        press=".onOpenViewSettings"                                                                                                                                                                                                            
                        icon="sap-icon://sort"                                                                                                                                                                                                                 
                        type="Emphasized"/>                                                                                                                                                                                                                    
                </OverflowToolbar>                                                                                                                                                                                                                             
            </headerToolbar>                                                                                                                                                                                                                                   
            <columns>                                                                                                                                                                                                                                          
                <Column id="columnLgnum">                                                                                                                                                                                                                      
                    <Text id="textLgnumColumn" text="{i18n>lineItemTableLgnumColumn}"/>                                                                                                                                                                        
                </Column>                                                                                                                                                                                                                                      
                <Column id="columnWerks">                                                                                                                                                                                                                      
                    <Text id="textWerksColumn" text="{i18n>lineItemTableWerksColumn}"/>                                                                                                                                                                        
                </Column>                                                                                                                                                                                                                                      
                <Column id="columnMatnr">                                                                                                                                                                                                                      
                    <Text id="textMatnrColumn" text="{i18n>lineItemTableMatnrColumn}"/>                                                                                                                                                                        
                </Column>                                                                                                                                                                                                                                      
                <Column id="columnAtivo">                                                                                                                                                                                                                      
                    <Text id="textAtivoColumn" text="{i18n>lineItemTableAtivoColumn}"/>                                                                                                                                                                        
                </Column>                                                                                                                                                                                                                                      
                <Column id="columnStatus">                                                                                                                                                                                                                     
                    <Text id="textStatusColumn" text="{i18n>lineItemTableStatusColumn}"/>                                                                                                                                                                      
                </Column>                                                                                                                                                                                                                                      
                <Column id="columnCriadoPor">                                                                                                                                                                                                                  
                    <Text id="textCriadoPorColumn" text="{i18n>lineItemTableCriadoPorColumn}"/>                                                                                                                                                                
                </Column>                                                                                                                                                                                                                                      
                <Column id="columnHrCriacao">                                                                                                                                                                                                                  
                    <Text id="textHrCriacaoColumn" text="{i18n>lineItemTableHrCriacaoColumn}"/>                                                                                                                                                                
                </Column>                                                                                                                                                                                                                                      
                <Column id="columnDtCriacao">                                                                                                                                                                                                                  
                    <Text id="textDtCriacaoColumn" text="{i18n>lineItemTableDtCriacaoColumn}"/>                                                                                                                                                                
                </Column>                                                                                                                                                                                                                                      
            </columns>                                                                                                                                                                                                                                         
            <items>                                                                                                                                                                                                                                            
                <ColumnListItem id="itemColumn" type="Detail" detailPress=".onBtnEditPress">                                                                                                                                                                   
                    <cells>                                                                                                                                                                                                                                    
                        <ObjectAttribute id="itemLgnum" text="{Lgnum}"/>                                                                                                                                                                                       
                        <ObjectAttribute id="itemWerks" text="{Werks}"/>                                                                                                                                                                                       
                        <ObjectAttribute id="itemMatnr" text="{Matnr}"/>                                                                                                                                                                                       
                        <HBox id="hboxAtivo">                                                                                                                                                                                                                  
                            <core:Icon id="iconAtivo"&nbsp;                                                                                                                                                                                                    
                                src="{                                                                                                                                                                                                                         
                                    path:'Ativo',                                                                                                                                                                                                              
                                    formatter:'.formatter.setIconStateAtivo'                                                                                                                                                                                   
                                }"&nbsp;                                                                                                                                                                                                                       
                                class="iconBold"                                                                                                                                                                                                               
                                size="1.5rem"                                                                                                                                                                                                                  
                                color="{                                                                                                                                                                                                                       
                                    path:'Ativo',&nbsp;                                                                                                                                                                                                        
                                    formatter: '.formatter.setIconColorAtivo'                                                                                                                                                                                  
                                }"                                                                                                                                                                                                                             
                            />                                                                                                                                                                                                                                 
                        </HBox>                                                                                                                                                                                                                                
                        <ObjectStatus id="itemteste"&nbsp;                                                                                                                                                                                                     
                            text="{                                                                                                                                                                                                                            
                                path:'Status',                                                                                                                                                                                                                 
                                formatter:'.formatter.setTextStatus'                                                                                                                                                                                           
                            }"                                                                                                                                                                                                                                 
                            state="{                                                                                                                                                                                                                           
                                path:'Status',                                                                                                                                                                                                                 
                                formatter:'.formatter.setTextColorStatus'                                                                                                                                                                                      
                            }"                                                                                                                                                                                                                                 
                            icon="{                                                                                                                                                                                                                            
                                path:'Status',                                                                                                                                                                                                                 
                                formatter:'.formatter.setIconStateStatus'&nbsp;                                                                                                                                                                                
                            }"                                                                                                                                                                                                                                 
                        />                                                                                                                                                                                                                                     
                        <ObjectAttribute id="itemCriadoPor" text="{CriadoPor}"/>                                                                                                                                                                               
                        <ObjectNumber id="itemHrCriacao" number="{ path:'HrCriacao', type:'sap.ui.model.odata.type.Time' }" />                                                                                                                                 
                        <ObjectNumber id="itemDtCriacao" number="{ path:'DtCriacao', type:'sap.ui.model.odata.type.Date' }" />                                                                                                                                 
                    </cells>                                                                                                                                                                                                                                   
                </ColumnListItem>                                                                                                                                                                                                                              
            </items>                                                                                                                                                                                                                                           
        </Table>                                                                                                                                                                                                                                               
    </Page>                                                                                                                                                                                                                                                    
</mvc:View>                                                                                                                                                                                                                                                    
                                                                                                                                                                                                                                                               