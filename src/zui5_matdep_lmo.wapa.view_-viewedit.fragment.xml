<core:FragmentDefinition                                                                                                                                                                                                                                       
	xmlns="sap.m"                                                                                                                                                                                                                                                 
	xmlns:core="sap.ui.core"&nbsp;&nbsp;&nbsp;                                                                                                                                                                                                                    
    xmlns:f="sap.ui.layout.form">                                                                                                                                                                                                                              
    <Dialog id="dialogEditMaterial"                                                                                                                                                                                                                            
        stretch="false"&nbsp;                                                                                                                                                                                                                                  
        resizable="true"&nbsp;                                                                                                                                                                                                                                 
        draggable="true"&nbsp;                                                                                                                                                                                                                                 
        title="{i18n>editar_material_title}">                                                                                                                                                                                                                  
        <content>&nbsp;                                                                                                                                                                                                                                        
            <f:SimpleForm                                                                                                                                                                                                                                      
                id="formEditMaterial"                                                                                                                                                                                                                          
                minWidth="1024"                                                                                                                                                                                                                                
                editable="false"                                                                                                                                                                                                                               
                layout="ResponsiveGridLayout"                                                                                                                                                                                                                  
                labelSpanL="3" labelSpanM="3" emptySpanL="4" emptySpanM="4"                                                                                                                                                                                    
                columnsL="1" columnsM="1">&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;                                                                     
                <f:content>                                                                                                                                                                                                                                    
                    <Label id="labelLgnumEdit" text="{i18n>lineItemTableLgnumColumn}"/>                                                                                                                                                                        
                    <Input id="inputLgnumEdit" type="Text" value="{oModelItens>/Lgnum}" editable="false"/>&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;     
                    <Label id="labelWerksEdit" text="{i18n>lineItemTableWerksColumn}"/>                                                                                                                                                                        
                    <Input id="inputWerksEdit" type="Text" value="{oModelItens>/Werks}" editable="false"/>&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;     
                    <Label id="labelMatnrEdit" text="{i18n>lineItemTableMatnrColumn}"/>                                                                                                                                                                        
                    <Input id="inputMatnrEdit" type="Text" value="{oModelItens>/Matnr}" editable="false"/>&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;     
                                                                                                                                                                                                                                                               
                    <Label id="labelAtivoEdit" text="{i18n>lineItemTableAtivoColumn}"/>                                                                                                                                                                        
                    <CheckBox id="inputAtivoEdit" selected="{oModelItens>/Ativo}"                                                                                                                                                                              
                        text="{                                                                                                                                                                                                                                
                            path:'oModelItens>/Ativo',&nbsp;                                                                                                                                                                                                   
                            formatter: '.formatter.setTextAtivo'                                                                                                                                                                                               
                        }"/>                                                                                                                                                                                                                                   
                </f:content>                                                                                                                                                                                                                                   
            </f:SimpleForm>                                                                                                                                                                                                                                    
        </content>&nbsp;                                                                                                                                                                                                                                       
        <buttons>                                                                                                                                                                                                                                              
            <Button id="btn_salvar_edit" text="{i18n>btn_salvar}" type="Emphasized" press=".onSaveMaterialButtonPress"/>					                                                                                                                                  
            <Button id="btn_voltar_edit" text="{i18n>btn_voltar}" type="Default" press=".onCancelMaterial"/>                                                                                                                                                   
        </buttons>	                                                                                                                                                                                                                                            
    </Dialog>                                                                                                                                                                                                                                                  
</core:FragmentDefinition>                                                                                                                                                                                                                                     