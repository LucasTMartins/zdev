<mvc:View                                                                                                                                                                                                                                                      
    xmlns:mvc="sap.ui.core.mvc"                                                                                                                                                                                                                                
    xmlns="sap.m"                                                                                                                                                                                                                                              
    xmlns:layout="sap.ui.layout.form"                                                                                                                                                                                                                          
    controllerName="br.com.lit.zui5.ne.logistics.offline.controller.ColetorTransfStock"                                                                                                                                                                        
    displayBlock="false"                                                                                                                                                                                                                                       
>                                                                                                                                                                                                                                                              
    <!-- Caixa para habilitar a rolagem -->                                                                                                                                                                                                                    
    <ScrollContainer                                                                                                                                                                                                                                           
        id="_IDGenScrollContainer1"                                                                                                                                                                                                                            
        width="100%"                                                                                                                                                                                                                                           
        height="100%"                                                                                                                                                                                                                                          
        vertical="true"                                                                                                                                                                                                                                        
        focusable="true"                                                                                                                                                                                                                                       
        class="sapUiNoContentPadding"                                                                                                                                                                                                                          
    >                                                                                                                                                                                                                                                          
        <VBox                                                                                                                                                                                                                                                  
            id="_IDGenVBox4"                                                                                                                                                                                                                                   
            class="sapUiSmallMargin"                                                                                                                                                                                                                           
        >                                                                                                                                                                                                                                                      
            <!-- Formulário -->                                                                                                                                                                                                                                
            <layout:SimpleForm                                                                                                                                                                                                                                 
                id="collectorForm"                                                                                                                                                                                                                             
                layout="ResponsiveGridLayout"                                                                                                                                                                                                                  
                editable="true"                                                                                                                                                                                                                                
                columnsL="2"                                                                                                                                                                                                                                   
                columnsM="1"                                                                                                                                                                                                                                   
                maxContainerCols="2"                                                                                                                                                                                                                           
            >                                                                                                                                                                                                                                                  
                <Label                                                                                                                                                                                                                                         
                    id="_IDGenLabel"                                                                                                                                                                                                                           
                    text="{i18n>columnWerks}"                                                                                                                                                                                                                  
                />                                                                                                                                                                                                                                             
                <Input                                                                                                                                                                                                                                         
                    id="idWerks"                                                                                                                                                                                                                               
                    maxLength="4"                                                                                                                                                                                                                              
                    width="5rem"                                                                                                                                                                                                                               
                    change="checkWerks"                                                                                                                                                                                                                        
&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;                                                                                                                                       
                />                                                                                                                                                                                                                                             
                                                                                                                                                                                                                                                               
                <Label                                                                                                                                                                                                                                         
                    id="_IDGenLabel1"                                                                                                                                                                                                                          
                    text="{i18n>columnLgnum}"                                                                                                                                                                                                                  
                />                                                                                                                                                                                                                                             
                <Input                                                                                                                                                                                                                                         
                    id="idLgnum"                                                                                                                                                                                                                               
                    maxLength="3"                                                                                                                                                                                                                              
                    width="4rem"                                                                                                                                                                                                                               
                    change="checkLgnum"                                                                                                                                                                                                                        
                />                                                                                                                                                                                                                                             
                                                                                                                                                                                                                                                               
                <Label                                                                                                                                                                                                                                         
                    id="_IDGenLabel3"                                                                                                                                                                                                                          
                    text="{i18n>columnVltyp}"                                                                                                                                                                                                                  
                />                                                                                                                                                                                                                                             
                <Input                                                                                                                                                                                                                                         
                    id="idVltyp"                                                                                                                                                                                                                               
                    maxLength="3"                                                                                                                                                                                                                              
                    width="4rem"                                                                                                                                                                                                                               
                    change="checkVltyp"                                                                                                                                                                                                                        
                />                                                                                                                                                                                                                                             
                                                                                                                                                                                                                                                               
                <Label                                                                                                                                                                                                                                         
                    id="_IDGenLabel2"                                                                                                                                                                                                                          
                    text="{i18n>columnVlpla}"                                                                                                                                                                                                                  
                />                                                                                                                                                                                                                                             
                <Input                                                                                                                                                                                                                                         
                    id="idVlpla"                                                                                                                                                                                                                               
                    change="checkVlpla"                                                                                                                                                                                                                        
                />                                                                                                                                                                                                                                             
                                                                                                                                                                                                                                                               
                <Label                                                                                                                                                                                                                                         
                    id="_IDGenLabel4"                                                                                                                                                                                                                          
                    text="{i18n>columnMatnr}"                                                                                                                                                                                                                  
                />                                                                                                                                                                                                                                             
                <Input                                                                                                                                                                                                                                         
                    id="idMatnr"                                                                                                                                                                                                                               
                    change="checkMatnr"                                                                                                                                                                                                                        
                />                                                                                                                                                                                                                                             
                                                                                                                                                                                                                                                               
                <Label                                                                                                                                                                                                                                         
                    id="_IDGenLabel5"                                                                                                                                                                                                                          
                    text="{i18n>columnMaktx}"                                                                                                                                                                                                                  
                />                                                                                                                                                                                                                                             
                <Input                                                                                                                                                                                                                                         
                    id="idMaktx"                                                                                                                                                                                                                               
                    editable="false"                                                                                                                                                                                                                           
                />                                                                                                                                                                                                                                             
                                                                                                                                                                                                                                                               
                <Label                                                                                                                                                                                                                                         
                    id="_IDGenLabel6"                                                                                                                                                                                                                          
                    text="{i18n>columnCharg}"                                                                                                                                                                                                                  
                />                                                                                                                                                                                                                                             
                <Input                                                                                                                                                                                                                                         
                    id="idCharg"                                                                                                                                                                                                                               
                    editable="false"                                                                                                                                                                                                                           
                    change="checkCharg"                                                                                                                                                                                                                        
                />                                                                                                                                                                                                                                             
                                                                                                                                                                                                                                                               
                <Label                                                                                                                                                                                                                                         
                    id="_IDGenLabel7"                                                                                                                                                                                                                          
                    text="{i18n>columnVerme}"                                                                                                                                                                                                                  
                />                                                                                                                                                                                                                                             
                <HBox id="_IDGenHBox">                                                                                                                                                                                                                         
                    <Input                                                                                                                                                                                                                                     
                        id="idVerme"                                                                                                                                                                                                                           
                        change="checkVerme"                                                                                                                                                                                                                    
                        type="Number"                                                                                                                                                                                                                          
                    />                                                                                                                                                                                                                                         
                    <Input                                                                                                                                                                                                                                     
                        id="idMeins"                                                                                                                                                                                                                           
                        editable="false"                                                                                                                                                                                                                       
                        width="3rem"                                                                                                                                                                                                                           
                    />                                                                                                                                                                                                                                         
                </HBox>                                                                                                                                                                                                                                        
                                                                                                                                                                                                                                                               
                <Label                                                                                                                                                                                                                                         
                    id="_IDGenLabel9"                                                                                                                                                                                                                          
                    text="{i18n>columnNltyp}"                                                                                                                                                                                                                  
                />                                                                                                                                                                                                                                             
                <Input                                                                                                                                                                                                                                         
                    id="idNltyp"                                                                                                                                                                                                                               
                    maxLength="3"                                                                                                                                                                                                                              
                    width="4rem"                                                                                                                                                                                                                               
                    change="checkNltyp"                                                                                                                                                                                                                        
                />                                                                                                                                                                                                                                             
                                                                                                                                                                                                                                                               
                <Label                                                                                                                                                                                                                                         
                    id="_IDGenLabel8"                                                                                                                                                                                                                          
                    text="{i18n>columnNlpla}"                                                                                                                                                                                                                  
                />                                                                                                                                                                                                                                             
                <Input                                                                                                                                                                                                                                         
                    id="idNlpla"                                                                                                                                                                                                                               
                    change="checkNlpla"                                                                                                                                                                                                                        
                />                                                                                                                                                                                                                                             
            </layout:SimpleForm>                                                                                                                                                                                                                               
                                                                                                                                                                                                                                                               
            <!-- Botões -->                                                                                                                                                                                                                                    
            <HBox                                                                                                                                                                                                                                              
                id="_IDGenHBox2"                                                                                                                                                                                                                               
                justifyContent="Center"                                                                                                                                                                                                                        
                class="sapUiMediumMarginTop"                                                                                                                                                                                                                   
            >                                                                                                                                                                                                                                                  
                <Button                                                                                                                                                                                                                                        
                    id="btnSave"                                                                                                                                                                                                                               
                    text="Gravar"                                                                                                                                                                                                                              
                    type="Accept"                                                                                                                                                                                                                              
                    press="onSave"                                                                                                                                                                                                                             
                    class="collectorButton"                                                                                                                                                                                                                    
                />                                                                                                                                                                                                                                             
                <Button                                                                                                                                                                                                                                        
                    id="btnClear"                                                                                                                                                                                                                              
                    text="{i18n>btnClear}"                                                                                                                                                                                                                     
                    type="Emphasized"                                                                                                                                                                                                                          
                    press="onClear"                                                                                                                                                                                                                            
                    class="sapUiTinyMarginBegin collectorButton"                                                                                                                                                                                               
                />                                                                                                                                                                                                                                             
            </HBox>                                                                                                                                                                                                                                            
        </VBox>                                                                                                                                                                                                                                                
    </ScrollContainer>                                                                                                                                                                                                                                         
</mvc:View>                                                                                                                                                                                                                                                    
                                                                                                                                                                                                                                                               