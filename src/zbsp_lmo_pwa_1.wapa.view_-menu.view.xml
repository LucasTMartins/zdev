<mvc:View                                                                                                                                                                                                                                                      
    controllerName="br.com.lit.zui5.ne.logistics.offline.controller.Menu"                                                                                                                                                                                      
    xmlns:mvc="sap.ui.core.mvc"                                                                                                                                                                                                                                
    displayBlock="true"                                                                                                                                                                                                                                        
    xmlns="sap.m"                                                                                                                                                                                                                                              
    xmlns:l="sap.ui.layout"                                                                                                                                                                                                                                    
>                                                                                                                                                                                                                                                              
    <Page                                                                                                                                                                                                                                                      
        id="menu"                                                                                                                                                                                                                                              
        title="Menu Inicial"                                                                                                                                                                                                                                   
    >                                                                                                                                                                                                                                                          
        <content>                                                                                                                                                                                                                                              
            <HBox id="installHeader">                                                                                                                                                                                                                          
                <Button                                                                                                                                                                                                                                        
                    id="installBtn"                                                                                                                                                                                                                            
                    text="Install"                                                                                                                                                                                                                             
                    visible="false"                                                                                                                                                                                                                            
                    press="installApp"                                                                                                                                                                                                                         
                />                                                                                                                                                                                                                                             
            </HBox>                                                                                                                                                                                                                                            
            <!-- Grupo: Relatórios -->                                                                                                                                                                                                                         
            <VBox                                                                                                                                                                                                                                              
                id="_IDGenVBox"                                                                                                                                                                                                                                
                class="sapUiSmallMargin"                                                                                                                                                                                                                       
            >                                                                                                                                                                                                                                                  
                <Title                                                                                                                                                                                                                                         
                    id="_IDGenTitle"                                                                                                                                                                                                                           
                    text="Relatórios"                                                                                                                                                                                                                          
                    level="H2"                                                                                                                                                                                                                                 
                    visible="true"                                                                                                                                                                                                                             
                />                                                                                                                                                                                                                                             
                <l:Grid                                                                                                                                                                                                                                        
                    id="_IDGenGrid"                                                                                                                                                                                                                            
                    defaultSpan="L2 M6 S12"                                                                                                                                                                                                                    
                    vSpacing="1"                                                                                                                                                                                                                               
                    hSpacing="1"                                                                                                                                                                                                                               
                >                                                                                                                                                                                                                                              
                    <!-- Primeiro Tile -->                                                                                                                                                                                                                     
                    <GenericTile                                                                                                                                                                                                                               
                        id="_IDGenGenericTile"                                                                                                                                                                                                                 
                        visible="{tileVisibility>/isLargeScreen}"                                                                                                                                                                                              
                        header="Saldo de Materiais"                                                                                                                                                                                                            
                        subheader="Verificação de Qtd"                                                                                                                                                                                                         
                        class="sapUiTinyMargin"                                                                                                                                                                                                                
                        press="pressReportBalanceStock"                                                                                                                                                                                                        
                    >                                                                                                                                                                                                                                          
                        <TileContent id="_IDGenTileContent">                                                                                                                                                                                                   
                            <NumericContent                                                                                                                                                                                                                    
                                id="_IDGenNumericContent"                                                                                                                                                                                                      
                                icon="sap-icon://documents"                                                                                                                                                                                                    
                                value="{countModel>/countBalanceStock}"                                                                                                                                                                                        
                                scale=""                                                                                                                                                                                                                       
                                withMargin="false"                                                                                                                                                                                                             
                            />                                                                                                                                                                                                                                 
                        </TileContent>                                                                                                                                                                                                                         
                    </GenericTile>                                                                                                                                                                                                                             
                                                                                                                                                                                                                                                               
                    <!-- Segundo Tile -->                                                                                                                                                                                                                      
                    <GenericTile                                                                                                                                                                                                                               
                        id="_IDGenGenericTile1"                                                                                                                                                                                                                
                        visible="{tileVisibility>/isLargeScreen}"                                                                                                                                                                                              
                        header="Posições"                                                                                                                                                                                                                      
                        class="sapUiTinyMargin"                                                                                                                                                                                                                
                        press="pressReportPosition"                                                                                                                                                                                                            
                    >                                                                                                                                                                                                                                          
                        <TileContent id="_IDGenTileContent1">                                                                                                                                                                                                  
                            <NumericContent                                                                                                                                                                                                                    
                                id="_IDGenNumericContent1"                                                                                                                                                                                                     
                                icon="sap-icon://documents"                                                                                                                                                                                                    
                                value="{countModel>/countPosition}"                                                                                                                                                                                            
                            />                                                                                                                                                                                                                                 
                        </TileContent>                                                                                                                                                                                                                         
                    </GenericTile>                                                                                                                                                                                                                             
                                                                                                                                                                                                                                                               
                    <!-- Terceiro Tile -->                                                                                                                                                                                                                     
                    <GenericTile                                                                                                                                                                                                                               
                        id="_IDGenGenericTile2"                                                                                                                                                                                                                
                        visible="{tileVisibility>/isLargeScreen}"                                                                                                                                                                                              
                        header="Tipos de Depósito"                                                                                                                                                                                                             
                        class="sapUiTinyMargin"                                                                                                                                                                                                                
                        press="pressReportTpDep"                                                                                                                                                                                                               
                    >                                                                                                                                                                                                                                          
                        <TileContent id="_IDGenTileContent2">                                                                                                                                                                                                  
                            <NumericContent                                                                                                                                                                                                                    
                                id="_IDGenNumericContent2"                                                                                                                                                                                                     
                                icon="sap-icon://documents"                                                                                                                                                                                                    
                                value="{countModel>/countTpDep}"                                                                                                                                                                                               
                            />                                                                                                                                                                                                                                 
                        </TileContent>                                                                                                                                                                                                                         
                    </GenericTile>                                                                                                                                                                                                                             
                                                                                                                                                                                                                                                               
                    <!-- Quarto Tile -->                                                                                                                                                                                                                       
                    <GenericTile                                                                                                                                                                                                                               
                        id="_IDGenGenericTile3"                                                                                                                                                                                                                
                        visible="{tileVisibility>/isSmallScreen}"                                                                                                                                                                                              
                        header="Depósitos"                                                                                                                                                                                                                     
                        class="sapUiTinyMargin"                                                                                                                                                                                                                
                        press="pressReportDeposit"                                                                                                                                                                                                             
                    >                                                                                                                                                                                                                                          
                        <TileContent id="_IDGenTileContent3">                                                                                                                                                                                                  
                            <NumericContent                                                                                                                                                                                                                    
                                id="_IDGenNumericContent3"                                                                                                                                                                                                     
                                icon="sap-icon://documents"                                                                                                                                                                                                    
                                value="{countModel>/countDeposit}"                                                                                                                                                                                             
                            />                                                                                                                                                                                                                                 
                        </TileContent>                                                                                                                                                                                                                         
                    </GenericTile>                                                                                                                                                                                                                             
                </l:Grid>                                                                                                                                                                                                                                      
            </VBox>                                                                                                                                                                                                                                            
                                                                                                                                                                                                                                                               
            <!-- Grupo: Cockpit -->                                                                                                                                                                                                                            
            <VBox                                                                                                                                                                                                                                              
                id="_IDGenVBox1"                                                                                                                                                                                                                               
                class="sapUiSmallMargin"                                                                                                                                                                                                                       
            >                                                                                                                                                                                                                                                  
                <Title                                                                                                                                                                                                                                         
                    id="_IDGenTitle1"                                                                                                                                                                                                                          
                    text="Cockpit"                                                                                                                                                                                                                             
                    level="H2"                                                                                                                                                                                                                                 
                    visible="{tileVisibility>/isSmallScreen}"                                                                                                                                                                                                  
                />                                                                                                                                                                                                                                             
                <l:Grid                                                                                                                                                                                                                                        
                    id="_IDGenGrid1"                                                                                                                                                                                                                           
                    defaultSpan="L3 M6 S12"                                                                                                                                                                                                                    
                    vSpacing="1"                                                                                                                                                                                                                               
                    hSpacing="1"                                                                                                                                                                                                                               
                >                                                                                                                                                                                                                                              
                    <!-- Primeiro Tile -->                                                                                                                                                                                                                     
                    <GenericTile                                                                                                                                                                                                                               
                        id="_IDGenGenericTile4"                                                                                                                                                                                                                
                        visible="{tileVisibility>/isSmallScreen}"                                                                                                                                                                                              
                        header="Transferência Posição"                                                                                                                                                                                                         
                        subheader="Materiais"                                                                                                                                                                                                                  
                        class="sapUiTinyMargin"                                                                                                                                                                                                                
                        press="pressCockpitTransfStock"                                                                                                                                                                                                        
                    >                                                                                                                                                                                                                                          
                        <TileContent id="_IDGenTileContent4">                                                                                                                                                                                                  
                            <NumericContent                                                                                                                                                                                                                    
                                id="_IDGenNumericContent4"                                                                                                                                                                                                     
                                icon="sap-icon://grid"                                                                                                                                                                                                         
                                value="{countModel>/countCockpitTransfStock}"                                                                                                                                                                                  
                            />                                                                                                                                                                                                                                 
                        </TileContent>                                                                                                                                                                                                                         
                    </GenericTile>                                                                                                                                                                                                                             
                </l:Grid>                                                                                                                                                                                                                                      
            </VBox>                                                                                                                                                                                                                                            
                                                                                                                                                                                                                                                               
            <!-- Grupo: Coletor -->                                                                                                                                                                                                                            
            <VBox                                                                                                                                                                                                                                              
                id="_IDGenVBox2"                                                                                                                                                                                                                               
                class="sapUiSmallMargin"                                                                                                                                                                                                                       
            >                                                                                                                                                                                                                                                  
                <Title                                                                                                                                                                                                                                         
                    id="_IDGenTitle2"                                                                                                                                                                                                                          
                    text="Coletor"                                                                                                                                                                                                                             
                    level="H2"                                                                                                                                                                                                                                 
                    visible="{tileVisibility>/isSmallScreen}"                                                                                                                                                                                                  
                />                                                                                                                                                                                                                                             
                <l:Grid                                                                                                                                                                                                                                        
                    id="_IDGenGrid2"                                                                                                                                                                                                                           
                    defaultSpan="L3 M6 S12"                                                                                                                                                                                                                    
                    vSpacing="1"                                                                                                                                                                                                                               
                    hSpacing="1"                                                                                                                                                                                                                               
                >                                                                                                                                                                                                                                              
                    <!-- Primeiro Tile -->                                                                                                                                                                                                                     
                    <GenericTile                                                                                                                                                                                                                               
                        id="_IDGenGenericTile5"                                                                                                                                                                                                                
                        header="Transferência de Posição"                                                                                                                                                                                                      
                        subheader="Materiais"                                                                                                                                                                                                                  
                        class="sapUiTinyMargin"                                                                                                                                                                                                                
                        press="pressColetorTransfStock"                                                                                                                                                                                                        
                        visible="{tileVisibility>/isSmallScreen}"                                                                                                                                                                                              
                    >                                                                                                                                                                                                                                          
                        <TileContent id="_IDGenTileContent5">                                                                                                                                                                                                  
                            <NumericContent                                                                                                                                                                                                                    
                                id="_IDGenNumericContent5"                                                                                                                                                                                                     
                                icon="sap-icon://grid"                                                                                                                                                                                                         
                                value="-"                                                                                                                                                                                                                      
                            />                                                                                                                                                                                                                                 
                        </TileContent>                                                                                                                                                                                                                         
                    </GenericTile>                                                                                                                                                                                                                             
                </l:Grid>                                                                                                                                                                                                                                      
            </VBox>                                                                                                                                                                                                                                            
        </content>                                                                                                                                                                                                                                             
    </Page>                                                                                                                                                                                                                                                    
</mvc:View>                                                                                                                                                                                                                                                    
                                                                                                                                                                                                                                                               