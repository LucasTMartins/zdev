sap.ui.define(["sap/ui/core/library"], function (coreLibrary) {                                                                                                                                                                                                
    "use strict"; var ValueState = coreLibrary.ValueState; return {                                                                                                                                                                                            
        // /**&nbsp;                                                                                                                                                                                                                                           
        //  * sap.ui.define(["sap/ui/core/library"], function (coreLibrary) {&nbsp;                                                                                                                                                                            
        //  * "use strict";   var ValueState = coreLibrary.ValueState;   return {&nbsp;                                                                                                                                                                        
        //  * Rounds the number unit value to 2 digits&nbsp;                                                                                                                                                                                                   
        //  * @public&nbsp;                                                                                                                                                                                                                                    
        //  * @param {string} sValue the number string to be rounded&nbsp;                                                                                                                                                                                     
        //  * @returns {string} sValue with 2 digits rounded **/                                                                                                                                                                                               
                                                                                                                                                                                                                                                               
        numberUnit: function (sValue) {                                                                                                                                                                                                                        
            if (!sValue) return "";                                                                                                                                                                                                                            
                                                                                                                                                                                                                                                               
            return parseFloat(sValue).toFixed(2);                                                                                                                                                                                                              
        },                                                                                                                                                                                                                                                     
                                                                                                                                                                                                                                                               
        stateStatus(sValue) {                                                                                                                                                                                                                                  
            if (sValue == "A") { return ValueState.Success; }                                                                                                                                                                                                  
            else if (sValue == "P") { return ValueState.Warning; }                                                                                                                                                                                             
            else if (sValue == "R") { return ValueState.Error; }                                                                                                                                                                                               
        },                                                                                                                                                                                                                                                     
                                                                                                                                                                                                                                                               
        iconStatus(sValue) {                                                                                                                                                                                                                                   
            if (sValue == "A") {                                                                                                                                                                                                                               
                return (sValue = "sap-icon://message-success");                                                                                                                                                                                                
            } else if (sValue == "R") {                                                                                                                                                                                                                        
                return (sValue = "sap-icon://error");                                                                                                                                                                                                          
            } else if (sValue == "P") {                                                                                                                                                                                                                        
                return (sValue = "sap-icon://alert");                                                                                                                                                                                                          
            }                                                                                                                                                                                                                                                  
        },                                                                                                                                                                                                                                                     
                                                                                                                                                                                                                                                               
        activeOrInactive(sValue) {                                                                                                                                                                                                                             
            // debugger;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;                                                                                                                                                                                             
            if (sValue == true) {                                                                                                                                                                                                                              
                return ValueState.Success;                                                                                                                                                                                                                     
            } else {                                                                                                                                                                                                                                           
                return ValueState.Error;                                                                                                                                                                                                                       
            }                                                                                                                                                                                                                                                  
        },                                                                                                                                                                                                                                                     
                                                                                                                                                                                                                                                               
        activeOrInactiveIcon(sValue) {                                                                                                                                                                                                                         
            // debugger;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;                                                                                                                                                                                             
            if (sValue == true) {                                                                                                                                                                                                                              
                return (sValue = "sap-icon://accept");                                                                                                                                                                                                         
            } else {                                                                                                                                                                                                                                           
                return (sValue = "sap-icon://decline");                                                                                                                                                                                                        
            }                                                                                                                                                                                                                                                  
        },                                                                                                                                                                                                                                                     
                                                                                                                                                                                                                                                               
        // activeOrInactiveStatus(sValue) {&nbsp;                                                                                                                                                                                                              
        //   // debugger;&nbsp;                                                                                                                                                                                                                                
        //   if (sValue == true) {&nbsp;                                                                                                                                                                                                                       
                                                                                                                                                                                                                                                               
        //     return (sValue = true);&nbsp;                                                                                                                                                                                                                   
        //   } else {&nbsp;                                                                                                                                                                                                                                    
        //     return (sValue = false);&nbsp;                                                                                                                                                                                                                  
        //   }&nbsp;                                                                                                                                                                                                                                           
        // },&nbsp;                                                                                                                                                                                                                                            
                                                                                                                                                                                                                                                               
        textStatus(sValue) {                                                                                                                                                                                                                                   
            if (sValue == "A") { return (sValue = "Aprovado"); }                                                                                                                                                                                               
            else if (sValue == "P") { return (sValue = "Pendente"); }                                                                                                                                                                                          
            else if (sValue == "R") { return (sValue = "Reprovado"); }                                                                                                                                                                                         
        },                                                                                                                                                                                                                                                     
                                                                                                                                                                                                                                                               
        removeZeroMaterial(sMaterial, sDescricao) {                                                                                                                                                                                                            
            if (sMaterial.indexOf("0")) {                                                                                                                                                                                                                      
                return sMaterial + " (" + sDescricao + ")";                                                                                                                                                                                                    
            } else {                                                                                                                                                                                                                                           
                //var str = sMaterial.replace("0", "");&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;                                                                                                                                                  
                var str = parseInt(sMaterial);                                                                                                                                                                                                                 
                return str + " (" + sDescricao + ")";                                                                                                                                                                                                          
            }                                                                                                                                                                                                                                                  
        },                                                                                                                                                                                                                                                     
                                                                                                                                                                                                                                                               
        descStatus: function (sMaterial) {                                                                                                                                                                                                                     
            if (sMaterial == "A") { return (sMaterial = "APROVADO"); }                                                                                                                                                                                         
            else if (sMaterial == "R") { return (sMaterial = "REPROVADO"); }                                                                                                                                                                                   
            else if (sMaterial == "P") { return (sMaterial = "PENDENTE"); }                                                                                                                                                                                    
        },                                                                                                                                                                                                                                                     
                                                                                                                                                                                                                                                               
        formatDate: function (sValue) {                                                                                                                                                                                                                        
            if (!sValue) {                                                                                                                                                                                                                                     
                return "";                                                                                                                                                                                                                                     
            }                                                                                                                                                                                                                                                  
                                                                                                                                                                                                                                                               
            var day = sValue.getDate();                                                                                                                                                                                                                        
            day = day + 1;                                                                                                                                                                                                                                     
                                                                                                                                                                                                                                                               
            if (day < 10) {                                                                                                                                                                                                                                    
                day = "0" + day;                                                                                                                                                                                                                               
            }                                                                                                                                                                                                                                                  
                                                                                                                                                                                                                                                               
            var month = sValue.getMonth();&nbsp;                                                                                                                                                                                                               
            month = month + 1;&nbsp;                                                                                                                                                                                                                           
                                                                                                                                                                                                                                                               
            if (month < 10) {                                                                                                                                                                                                                                  
                month = "0" + month;                                                                                                                                                                                                                           
            }&nbsp;                                                                                                                                                                                                                                            
            var year = sValue.getFullYear();                                                                                                                                                                                                                   
                                                                                                                                                                                                                                                               
            return day + "." + month + "." + year;                                                                                                                                                                                                             
        },&nbsp;                                                                                                                                                                                                                                               
&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;                                                                                                                                                                                                               
        formatHour: function (sValue) { },                                                                                                                                                                                                                     
    };                                                                                                                                                                                                                                                         
});                                                                                                                                                                                                                                                            
                                                                                                                                                                                                                                                               
                                                                                                                                                                                                                                                               