<mvc:View                                                                                                                                                                                                                                                      
    xmlns:mvc="sap.ui.core.mvc"                                                                                                                                                                                                                                
    xmlns="sap.m"                                                                                                                                                                                                                                              
    controllerName="br.com.lit.zui5.ne.logistics.offline.controller.ReportTpDep"                                                                                                                                                                               
>                                                                                                                                                                                                                                                              
    <Page id="idPageTpDep">                                                                                                                                                                                                                                    
        <customHeader>                                                                                                                                                                                                                                         
            <Bar id="_IDGenBar">                                                                                                                                                                                                                               
                <contentMiddle>                                                                                                                                                                                                                                
                    <Title                                                                                                                                                                                                                                     
                        id="_IDGenTitle4"                                                                                                                                                                                                                      
                        text="{i18n>titleReportTpDep}"                                                                                                                                                                                                         
                    />                                                                                                                                                                                                                                         
                </contentMiddle>                                                                                                                                                                                                                               
            </Bar>                                                                                                                                                                                                                                             
        </customHeader>                                                                                                                                                                                                                                        
        <content>                                                                                                                                                                                                                                              
            <VBox                                                                                                                                                                                                                                              
                id="_IDGenVBox3"                                                                                                                                                                                                                               
                class="sapUiSmallMargin"                                                                                                                                                                                                                       
            >                                                                                                                                                                                                                                                  
                <Table                                                                                                                                                                                                                                         
                    id="idTiposDepositoTable"                                                                                                                                                                                                                  
                    items="{ReportTpDep>/}"                                                                                                                                                                                                                    
                    alternateRowColors="true"                                                                                                                                                                                                                  
                    fixedLayout="false"                                                                                                                                                                                                                        
                    growing="true"                                                                                                                                                                                                                             
                    growingScrollToLoad="true"                                                                                                                                                                                                                 
                >                                                                                                                                                                                                                                              
                    <columns>                                                                                                                                                                                                                                  
                        <Column                                                                                                                                                                                                                                
                            id="_IDGenColumn14"                                                                                                                                                                                                                
                            hAlign="Center"                                                                                                                                                                                                                    
                        >                                                                                                                                                                                                                                      
                            <Text                                                                                                                                                                                                                              
                                id="_IDGenText18"                                                                                                                                                                                                              
                                text="{i18n>columnId}"                                                                                                                                                                                                         
                            />                                                                                                                                                                                                                                 
                        </Column>                                                                                                                                                                                                                              
                        <Column                                                                                                                                                                                                                                
                            id="_IDGenColumn15"                                                                                                                                                                                                                
                            hAlign="Center"                                                                                                                                                                                                                    
                        >                                                                                                                                                                                                                                      
                            <Text                                                                                                                                                                                                                              
                                id="_IDGenText25"                                                                                                                                                                                                              
                                text="{i18n>columnLgtyp}"                                                                                                                                                                                                      
                            />                                                                                                                                                                                                                                 
                        </Column>                                                                                                                                                                                                                              
                        <Column                                                                                                                                                                                                                                
                            id="_IDGenColumn16"                                                                                                                                                                                                                
                            hAlign="Center"                                                                                                                                                                                                                    
                        >                                                                                                                                                                                                                                      
                            <Text                                                                                                                                                                                                                              
                                id="_IDGenText26"                                                                                                                                                                                                              
                                text="{i18n>columnLgnum}"                                                                                                                                                                                                      
                            />                                                                                                                                                                                                                                 
                        </Column>                                                                                                                                                                                                                              
                        <Column                                                                                                                                                                                                                                
                            id="_IDGenColumn17"                                                                                                                                                                                                                
                            hAlign="Center"                                                                                                                                                                                                                    
                        >                                                                                                                                                                                                                                      
                            <Text                                                                                                                                                                                                                              
                                id="_IDGenText27"                                                                                                                                                                                                              
                                text="{i18n>columnDate}"                                                                                                                                                                                                       
                            />                                                                                                                                                                                                                                 
                        </Column>                                                                                                                                                                                                                              
                        <Column                                                                                                                                                                                                                                
                            id="_IDGenColumn18"                                                                                                                                                                                                                
                            hAlign="Center"                                                                                                                                                                                                                    
                        >                                                                                                                                                                                                                                      
                            <Text                                                                                                                                                                                                                              
                                id="_IDGenText28"                                                                                                                                                                                                              
                                text="{i18n>columnHour}"                                                                                                                                                                                                       
                            />                                                                                                                                                                                                                                 
                        </Column>                                                                                                                                                                                                                              
                        <Column                                                                                                                                                                                                                                
                            id="_IDGenColumn19"                                                                                                                                                                                                                
                            hAlign="Center"                                                                                                                                                                                                                    
                        >                                                                                                                                                                                                                                      
                            <Text                                                                                                                                                                                                                              
                                id="_IDGenText29"                                                                                                                                                                                                              
                                text="{i18n>columnStatus}"                                                                                                                                                                                                     
                            />                                                                                                                                                                                                                                 
                        </Column>                                                                                                                                                                                                                              
                    </columns>                                                                                                                                                                                                                                 
                    <items>                                                                                                                                                                                                                                    
                        <ColumnListItem id="idColumnListItem">                                                                                                                                                                                                 
                            <cells>                                                                                                                                                                                                                            
                                <Text                                                                                                                                                                                                                          
                                    id="_IDGenText30"                                                                                                                                                                                                          
                                    textAlign="Center"                                                                                                                                                                                                         
                                    text="{ReportTpDep>id}"                                                                                                                                                                                                    
                                />                                                                                                                                                                                                                             
                                <Text                                                                                                                                                                                                                          
                                    id="_IDGenText31"                                                                                                                                                                                                          
                                    textAlign="Center"                                                                                                                                                                                                         
                                    text="{ReportTpDep>lgtyp}"                                                                                                                                                                                                 
                                />                                                                                                                                                                                                                             
                                <Text                                                                                                                                                                                                                          
                                    id="_IDGenText32"                                                                                                                                                                                                          
                                    textAlign="Center"                                                                                                                                                                                                         
                                    text="{ReportTpDep>lgnum/lgnum}"                                                                                                                                                                                           
                                />                                                                                                                                                                                                                             
                                <Text                                                                                                                                                                                                                          
                                    id="_IDGenText33"                                                                                                                                                                                                          
                                    textAlign="Center"                                                                                                                                                                                                         
                                    text="{ReportTpDep>crdate}"                                                                                                                                                                                                
                                />                                                                                                                                                                                                                             
                                <Text                                                                                                                                                                                                                          
                                    id="_IDGenText34"                                                                                                                                                                                                          
                                    textAlign="Center"                                                                                                                                                                                                         
                                    text="{ReportTpDep>crtime}"                                                                                                                                                                                                
                                />                                                                                                                                                                                                                             
                                <ObjectStatus                                                                                                                                                                                                                  
                                    id="_IDGenObjectStatus"                                                                                                                                                                                                    
                                    text=""                                                                                                                                                                                                                    
                                    icon="{= ${ReportTpDep>active} ? 'sap-icon://accept' : 'sap-icon://decline'}"                                                                                                                                              
                                    state="{= ${ReportTpDep>active} ? 'Success' : 'Error'}"                                                                                                                                                                    
                                />                                                                                                                                                                                                                             
                            </cells>                                                                                                                                                                                                                           
                        </ColumnListItem>                                                                                                                                                                                                                      
                    </items>                                                                                                                                                                                                                                   
                </Table>                                                                                                                                                                                                                                       
            </VBox>                                                                                                                                                                                                                                            
        </content>                                                                                                                                                                                                                                             
    </Page>                                                                                                                                                                                                                                                    
</mvc:View>                                                                                                                                                                                                                                                    
                                                                                                                                                                                                                                                               