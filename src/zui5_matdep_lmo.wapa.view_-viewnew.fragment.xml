<core:FragmentDefinition                                                                                                                                                                                                                                       
	xmlns="sap.m"                                                                                                                                                                                                                                                 
	xmlns:core="sap.ui.core"&nbsp;&nbsp;&nbsp;                                                                                                                                                                                                                    
    xmlns:f="sap.ui.layout.form">                                                                                                                                                                                                                              
    <Dialog id="dialogNewMaterial"                                                                                                                                                                                                                             
        stretch="false"&nbsp;                                                                                                                                                                                                                                  
        resizable="true"&nbsp;                                                                                                                                                                                                                                 
        draggable="true"&nbsp;                                                                                                                                                                                                                                 
        title="{i18n>nova_material_title}">                                                                                                                                                                                                                    
        <content>&nbsp;                                                                                                                                                                                                                                        
            <f:SimpleForm                                                                                                                                                                                                                                      
                id="formCreateMaterial"                                                                                                                                                                                                                        
                minWidth="1024"                                                                                                                                                                                                                                
                editable="false"                                                                                                                                                                                                                               
                layout="ResponsiveGridLayout"                                                                                                                                                                                                                  
                labelSpanL="3" labelSpanM="3" emptySpanL="4" emptySpanM="4"                                                                                                                                                                                    
                columnsL="1" columnsM="1">&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;                                                                     
                <f:content>                                                                                                                                                                                                                                    
                    <Label id="labelLgnumNew" text="{i18n>lineItemTableLgnumColumn}"/>                                                                                                                                                                         
                    <Input id="inputLgnumNew" type="Text" value="{Lgnum}"/>&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;                                    
                    <Label id="labelWerksNew" text="{i18n>lineItemTableWerksColumn}"/>                                                                                                                                                                         
                    <Input id="inputWerksNew" type="Text" value="{Werks}"/>&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;                                    
                    <Label id="labelMatnrNew" text="{i18n>lineItemTableMatnrColumn}"/>                                                                                                                                                                         
                    <Input id="inputMatnrNew" type="Text" value="{Matnr}"/>&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;                                    
                </f:content>                                                                                                                                                                                                                                   
            </f:SimpleForm>                                                                                                                                                                                                                                    
        </content>&nbsp;                                                                                                                                                                                                                                       
        <buttons>                                                                                                                                                                                                                                              
            <Button id="btn_salvar_new" text="{i18n>btn_salvar}" type="Emphasized" press=".onSaveMaterialButtonPress"/>					                                                                                                                                   
            <Button id="btn_voltar_new" text="{i18n>btn_voltar}" type="Default" press=".onCancelMaterial"/>                                                                                                                                                    
        </buttons>	                                                                                                                                                                                                                                            
    </Dialog>                                                                                                                                                                                                                                                  
</core:FragmentDefinition>                                                                                                                                                                                                                                     