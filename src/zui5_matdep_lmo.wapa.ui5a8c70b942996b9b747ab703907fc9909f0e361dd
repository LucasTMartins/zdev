sap.ui.define([],&nbsp;                                                                                                                                                                                                                                        
function() {                                                                                                                                                                                                                                                   
    'use strict';                                                                                                                                                                                                                                              
                                                                                                                                                                                                                                                               
    return{                                                                                                                                                                                                                                                    
        setIconStateAtivo: function(Ativo) {                                                                                                                                                                                                                   
            return Ativo === true ? "sap-icon://accept" : "sap-icon://decline";                                                                                                                                                                                
        },                                                                                                                                                                                                                                                     
                                                                                                                                                                                                                                                               
        setIconColorAtivo: function(Ativo){                                                                                                                                                                                                                    
            return Ativo === true ? "Positive" : "Negative";                                                                                                                                                                                                   
        },                                                                                                                                                                                                                                                     
                                                                                                                                                                                                                                                               
        setIconStateStatus: function(Status) {                                                                                                                                                                                                                 
            switch (Status) {                                                                                                                                                                                                                                  
                case "A":                                                                                                                                                                                                                                      
                    return "sap-icon://sys-enter-2";                                                                                                                                                                                                           
                case "P":                                                                                                                                                                                                                                      
                    return "sap-icon://alert";                                                                                                                                                                                                                 
                case "R":                                                                                                                                                                                                                                      
                    return "sap-icon://error";                                                                                                                                                                                                                 
                default:                                                                                                                                                                                                                                       
                    break;                                                                                                                                                                                                                                     
            }                                                                                                                                                                                                                                                  
        },                                                                                                                                                                                                                                                     
                                                                                                                                                                                                                                                               
        setTextStatus: function(Status){                                                                                                                                                                                                                       
            switch (Status) {                                                                                                                                                                                                                                  
                case "A":                                                                                                                                                                                                                                      
                    return "Aprovado";                                                                                                                                                                                                                         
                case "P":                                                                                                                                                                                                                                      
                    return "Pendente";                                                                                                                                                                                                                         
                case "R":                                                                                                                                                                                                                                      
                    return "Reprovado";                                                                                                                                                                                                                        
                default:                                                                                                                                                                                                                                       
                    break;                                                                                                                                                                                                                                     
            }                                                                                                                                                                                                                                                  
        },                                                                                                                                                                                                                                                     
                                                                                                                                                                                                                                                               
        setTextColorStatus: function(Status){                                                                                                                                                                                                                  
            switch (Status) {                                                                                                                                                                                                                                  
                case "A":                                                                                                                                                                                                                                      
                    return "Success";                                                                                                                                                                                                                          
                case "P":                                                                                                                                                                                                                                      
                    return "Warning";                                                                                                                                                                                                                          
                case "R":                                                                                                                                                                                                                                      
                    return "Error";                                                                                                                                                                                                                            
                default:                                                                                                                                                                                                                                       
                    break;                                                                                                                                                                                                                                     
            }                                                                                                                                                                                                                                                  
        },                                                                                                                                                                                                                                                     
                                                                                                                                                                                                                                                               
        setTextAtivo: function(Ativo){                                                                                                                                                                                                                         
            switch (Ativo) {                                                                                                                                                                                                                                   
                case true:                                                                                                                                                                                                                                     
                    return "Ativo";                                                                                                                                                                                                                            
                case false:                                                                                                                                                                                                                                    
                    return "Não ativo";                                                                                                                                                                                                                        
                default:                                                                                                                                                                                                                                       
                    break;                                                                                                                                                                                                                                     
            }                                                                                                                                                                                                                                                  
        },                                                                                                                                                                                                                                                     
    }                                                                                                                                                                                                                                                          
});                                                                                                                                                                                                                                                            